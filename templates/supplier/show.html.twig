<!DOCTYPE html>

<title>Supplier</title>

{% block body %}
    <h1>Supplier</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ supplier.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ supplier.name }}</td>
            </tr>
            <tr>
                <th>Website</th>
                <td>{{ supplier.website }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ supplier.createdAt ? supplier.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ supplier.updatedAt ? supplier.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('super_admin_app_supplier_index') }}">back to list</a>

    <a href="{{ path('super_admin_app_supplier_edit', {'id': supplier.id}) }}">edit</a>

    {{ include('supplier/_delete_form.html.twig') }}
{% endblock %}
