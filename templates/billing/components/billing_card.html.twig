<twig:Card>
<section>
        <div class="flex justify-between">
        <div>
                <p class="font-semibold text-2xl">{{ title }}</p>
                <p class="text-sm">{{ description }}</p>
        </div>
            <twig:RoundedIcon
                icon="fa fa-plus"
                route="{{ routeIcon }}"
            />   
        </div>
    </section>
    <section>
        {{block(outerBlocks.content)}}
    </section>
</twig:Card>
