<form method="post" action="{{ path('super_admin_app_company_delete', {'id': company.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ company.id) }}">
    <button class="w-8 h-8 flex items-center justify-center bg-error text-xs font-bold uppercase rounded outline-none focus:outline-none ml-auto mr-1 mb-1 ease-linear transition-all duration-150 rounded-full">
        <i class="fa-solid fa-trash-can"></i>
    </button>
</form>
